provider "aws" {
  region = "${aws_region}"

  access_key = "test"
  secret_key = "test"

  skip_credentials_validation = true
  skip_metadata_api_check     = true
  skip_requesting_account_id  = true

  s3_use_path_style = true

  endpoints {
    %{ for svc in localstack_services ~}
    ${svc} = "${localstack_endpoint}"
    %{ endfor ~}
  }
}
